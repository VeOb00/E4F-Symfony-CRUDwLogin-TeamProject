{% extends 'base.html.twig' %}

{% block title %}Members{% endblock %}

{% block body %}
    <h1>Business single post</h1>

    <table class="table">
        <tbody>
        <tr>
            <th>Id</th>
            <td>{{ business.id }}</td>
        </tr>
        <tr>
            <th>Name</th>
            <td>{{ business.name }}</td>
        </tr>
        <tr>
            <th>Branch</th>
            <td>{{ business.branch }}</td>
        </tr>
        <tr>
            <th>NrEmployees</th>
            <td>{{ business.nrEmployees }}</td>
        </tr>
        <tr>
            <th>StrNameNr</th>
            <td>{{ business.strNameNr }}</td>
        </tr>
        <tr>
            <th>City</th>
            <td>{{ business.city }}</td>
        </tr>
        <tr>
            <th>Zipcode</th>
            <td>{{ business.zipcode }}</td>
        </tr>
        <tr>
            <th>Country</th>
            <td>{{ business.country }}</td>
        </tr>
        <tr>
            <th>EMail</th>
            <td>{{ business.eMail }}</td>
        </tr>
        <tr>
            <th>Webpage</th>
            <td>{{ business.webpage }}</td>
        </tr>
        <tr>
            <th>TelNr</th>
            <td>{{ business.telNr }}</td>
        </tr>
        <tr>
            <th>Logo</th>
            <td>{{ business.logo }}</td>
        </tr>
        <tr>
            <th>Image</th>
            <td>{{ business.image }}</td>
        </tr>
        <tr>
            <th>Description</th>
            <td>{{ business.description }}</td>
        </tr>
        <tr>
            <th>Moto</th>
            <td>{{ business.moto }}</td>
        </tr>
        </tbody>
    </table>

    <a href="{{ path('members') }}">back to list</a>



{% endblock %}
